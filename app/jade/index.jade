html.no-js(ng-app="DSElectronicsApp")
  //<![endif]
  head
    meta(charset="utf-8")
    meta(http-equiv="X-UA-Compatible", content="IE=edge,chrome=1")
    title
    meta(name="description", content="")
    meta(name="viewport", content="width=device-width")
    // Place favicon.ico and apple-touch-icon.png in the root directory
    
    // build:css(.tmp) styles/main.css 
    link(rel="stylesheet", href="styles/main.css")
    // endbuild
    //- build:head
  body(ng-controller='mainCtrl')
    //[if lt IE 7]>
      <p class="chromeframe">You are using an outdated browser. <a href="http://browsehappy.com/">Upgrade your browser today</a> or <a href="http://www.google.com/chromeframe/?redirect=true">install Google Chrome Frame</a> to better experience this site.</p>
      <![endif]
    //[if lt IE 9]>
      <script src="bower_components/es5-shim/es5-shim.js"></script>
      <script src="bower_components/json3/lib/json3.min.js"></script>
      <![endif]

    // Add your site or application content here
    nav.navbar.navbar-main.navbar-inverse.navbar-fixed-top(role='navigation', ng-controller='navMenu')
      .navbar-header
        button.navbar-toggle.collapsed(type='button', data-toggle='collapse', data-target='.navbar-main-collapse')
          span.sr-only Toggle Navigation
          span.glyphicon.glyphicon-tasks
        a.navbar-brand(href='javascript:void(0)') DS Electronics 
      .navbar-main-collapse.navbar-collapse.collapse
        ul.nav.navbar-nav
          li.dropdown.active
            a(href='javascript:void(0)', data-toggle='dropdown') 
              | Views 
              b.caret
            ul.dropdown-menu(role='menu', aria-labelledby='dropdownMenuViews')
              li.active
                a(href='javascript:void(0)', role='menuitem', tabindex='-1') Inventory Items
              li
                a(href='javascript:void(0)', role='menuitem', tabindex='-1') Vendors
              li
                a(href='javascript:void(0)', role='menuitem', tabindex='-1') Kits
              li.divider.visible-xs   
          li
            a(href='javascript:void(0)') Research
          li
            a(href='javascript:void(0)') Tools

        ul.nav.navbar-nav.navbar-right
          li.divider.visible-xs(class='{{authenticated|loginButton:true}}')
          li
            a(href='javascript:void(0)', ng-init="login='Login'", class='{{authenticated|loginButton:true}}', ng-bind='login', ng-click='loginModal()')
          li.divider.visible-xs(class='{{authenticated|loginButton:false}}')
          li
            a(href="javascript:void(0)", ng-init="logout='Logout'", class='{{authenticated|loginButton:false}}', ng-bind='logout', ng-click='logoutModal()')

    #wrap
      .main.container(ng-view)
      
      #push

    footer
      p.text-muted.text-center
        - var date = new Date, date = date.getFullYear();
        | &copy; #{date} DS Electronics, all rights reserved. Site proudly crafted by Digitally Seamless

    // ### bootstrap modals ###
    // login
    #loginModal.modal.fade(role='dialog', tabindex='-1', aria-labelledby='loginModal', aria-hidden='true', ng-controller='loginModal')
      .modal-dialog
        .modal-content
          .modal-header
            button.close(type='button', data-dismiss='modal', aria-hidden='true', ng-click='loginForm.password=""') &times;
            h4.modal-title Login
          .modal-body
            // login form goes here
            form.form-horizontal(role='form')
              .form-group
                label.col-lg-2.control-label(for='loginUsername') Username
                .col-lg-10
                  input#loginUsername.form-control(type='text', placeholder='username', ng-model='loginForm.username')
              .form-group
                label.col-lg-2.control-label(for='loginPassword') Password
                .col-lg-10
                  input#loginPassword.form-control(type='password', placeholder='password', ng-model='loginForm.password')
              .form-group
                .col-lg-offset-2.col-lg-10
                  .checkbox
                    label
                      input(type='checkbox', ng-model='loginForm.remember')
                      | Remember me
              

          .modal-footer
            button.btn.btn-default(type='button', data-dismiss='modal', ng-click='loginForm.password=""') cancel
            button.btn.btn-primary(type='button', ng-click='login()') login

    // Google Analytics: change UA-XXXXX-X to be your site's ID.
    script.
      var _gaq=[['_setAccount','UA-XXXXX-X'],['_trackPageview']];
      (function(d,t){var g=d.createElement(t),s=d.getElementsByTagName(t)[0];
      g.src='//www.google-analytics.com/ga.js';
      s.parentNode.insertBefore(g,s)}(document,'script'));


    script(src="bower_components/jquery/jquery.js")
    script(src="bower_components/angular/angular.js")
    script(src="bower_components/angular-resource/angular-resource.js")
    script(src="bower_components/angular-cookies/angular-cookies.js")
    script(src="bower_components/angular-sanitize/angular-sanitize.js")

    
    // build:js scripts/plugins.js 
    script(src="bower_components/bootstrap/js/affix.js")
    script(src="bower_components/bootstrap/js/alert.js")
    script(src="bower_components/bootstrap/js/dropdown.js")
    script(src="bower_components/bootstrap/js/tooltip.js")
    script(src="bower_components/bootstrap/js/modal.js")
    script(src="bower_components/bootstrap/js/transition.js")
    script(src="bower_components/bootstrap/js/button.js")
    script(src="bower_components/bootstrap/js/popover.js")
    script(src="bower_components/bootstrap/js/carousel.js")
    script(src="bower_components/bootstrap/js/scrollspy.js")
    script(src="bower_components/bootstrap/js/collapse.js")
    script(src="bower_components/bootstrap/js/tab.js")
    // endbuild
    
    // build:js scripts/plugins.js 
    script(src="scripts/socket.io/socket.io.js")
    // endbuild
    
    // build:js({.tmp,app}) scripts/scripts.js 
    script(src="scripts/app.js")
    script(src="scripts/session/main.js")
    script(src="scripts/controllers/main.js")
    script(src="scripts/filters/main.js")
    // endbuild
    //- build:body
